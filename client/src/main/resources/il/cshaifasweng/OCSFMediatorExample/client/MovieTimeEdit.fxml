<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.EditTimeController">
   <children>
      <TableView fx:id="ShowTimeTable" layoutX="14.0" layoutY="14.0" onMouseClicked="#getSelected" prefHeight="201.0" prefWidth="512.0">
        <columns>
          <TableColumn fx:id="showidCol" prefWidth="75.0" text="Showid" />
          <TableColumn fx:id="showTimeCol" prefWidth="75.0" text="Time" />
        </columns>
      </TableView>
      <TextField fx:id="showtTimeTextBox" layoutX="28.0" layoutY="244.0" prefHeight="30.0" prefWidth="205.0" />
      <Button fx:id="UpdateButton" layoutX="35.0" layoutY="300.0" mnemonicParsing="false" onAction="#updateSelectedShowTime" prefHeight="30.0" prefWidth="78.0" text="Update" />
   </children>
</AnchorPane>
